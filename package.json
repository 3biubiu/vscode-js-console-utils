{
    "name": "console-log-utils",
    "displayName": "console.log utils",
    "description": "Helps nudge console.log statements around",
    "version": "0.0.1",
    "publisher": "whtouche",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.insertLogStatement",
        "onCommand:extension.deleteAllConsoleLogs"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.insertLogStatement",
            "title": "Insert Log Statement for Variable"
        },{
            "command": "extension.deleteAllConsoleLogs",
            "title": "Delete all console.log statements"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}